{
    "name": "poweredlocal/vrata",
    "description": "API gateway implemented in PHP and Lumen",
    "keywords": ["microservices","api gateway","lumen"],
    "license": "MIT",
    "type": "project",
    "homepage": "https://github.com/PoweredLocal/vrata",
    "authors": [
        {
            "name": "Denis Mysenko",
            "email": "denis@poweredlocal.com",
            "homepage": "https://www.poweredlocal.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/lumen-framework": "5.3.*",
        "vlucas/phpdotenv": "~2.2",
        "laravel/passport": "^0.2.2",
        "appzcoder/lumen-routes-list": "^1.0",
        "dusterio/lumen-passport": "^0.1",
        "guzzlehttp/guzzle": "~6.0",
        "webpatser/laravel-uuid": "2.*",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
	"codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/",
            "database/"
        ]
    },
    "prefer-stable": true
}
